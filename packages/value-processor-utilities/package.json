{
    "name": "@gbt/value-processor-utilities",
    "description": "Value Processor Utilities",
    "version": "0.0.2",
    "private": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md"
    ],
    "exports": {
        ".": [
            "./dist/GBT_Value_Processor_Utilities.sbsar"
        ]
    },
    "main": "dist/GBT_Value_Processor_Utilities.sbsar",
    "keywords": [
        "typescript",
        "python",
        "sat",
        "psybs",
        "utilities"
    ],
    "scripts": {
        "\n========== BUILD >> ==========": "",
        "build": "mkdir -p dist && pnpm build:sbs",
        "build:sbs": "/Users/gilliantunney/Substance\\ 12/Adobe\\ Substance\\ 3D\\ Designer.app/Contents/MacOS/sbscooker --inputs ./src/*.sbs --includes /Users/gilliantunney/Substance\\ 12/packages --quiet --size-limit 13 --output-path ./dist",
        "build:ts": "tsc --build",
        "\n========== DEV >> ==========": "",
        "dev": "tsc --build --watch",
        "\n========== TEST >> ==========": "",
        "test": "pnpm build",
        "\n========== PUBLISH >> ==========": "",
        "release": "pnpm prerelease && pnpm publish --access public --report-summary --no-git-checks",
        "prerelease": "pnpm clean && pnpm build && pnpm run docs && pnpm fix && pnpm check && pnpm test",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:build",
        "clean:build": "rm -R -f dist",
        "\n========== CODE STYLE >> ==========": "",
        "prettier": "cross-var prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions} --write --ignore-path .gitignore",
        "\n========== CHECK >> ==========": "",
        "check": "pnpm build:ts && pnpm clean:ts",
        "\n========== FIX >> ==========": "",
        "fix": "pnpm prettier"
    },
    "license": "MIT",
    "author": {
        "name": "Gillian Tunney",
        "email": "gbtunney@mac.com"
    },
    "devDependencies": {
        "@prettier/plugin-xml": "^2.2.0",
        "@snailicide/build-config": "^0.2.5",
        "@snailicide/g-library": "^0.1.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "python-shell": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "config": {
        "eslint": {
            "extensions": "ts,js,cjs,mjs",
            "path": ".es*,*,**/*",
            "config": ".eslintrc.cjs"
        },
        "prettier": {
            "extensions": "sbs,ts,js,cjs,mjs,cts,json,yaml,yml,graphql,md",
            "path": "*,**/*"
        }
    }
}

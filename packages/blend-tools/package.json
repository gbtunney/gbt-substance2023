{
    "name": "@gbt/blend-tools",
    "description": "SD Blend Tools",
    "version": "0.0.0",
    "private": false,
    "type": "module",
    "files": [
        "dist",
        "CHANGELOG.md"
    ],
    "exports": {
        ".": [
            "./dist/GBT_Blend_Tools.sbsar"
        ]
    },
    "main": "dist/GBT_Blend_Tools.sbsar",
    "keywords": [
        "typescript",
        "python",
        "sat",
        "psybs",
        "utilities"
    ],
    "scripts": {
        "\n========== BUILD >> ==========": "",
        "copy:deps": "mkdir -p examples/dependencies && cp node_modules/@gbt/value-processor-utilities/dist/* examples/dependencies",
        "build": "pnpm build:ts && pnpm build:sbs && mkdir -p dist && pnpm build:sbsar",
        "build:sbsar": "/Users/gilliantunney/Substance\\ 12/Adobe\\ Substance\\ 3D\\ Designer.app/Contents/MacOS/sbscooker --inputs ./_temp/*.sbs --verbose --merge --output-name $npm_package_main",
        "build:sbs": "pnpm exec sdbuilder --rootDir $PWD --outDir ./_temp --inputSBS 'src/*.sbs' --inputData 'meta/fileReplaceSpecScript.js' --debug --overwrite ",
        "build:ts": "tsc --build",
        "\n========== DEV >> ==========": "",
        "dev": "tsc --build --watch",
        "\n========== TEST >> ==========": "",
        "test:pwd": "echo $npm_package_main;",
        "test:cli": "pnpm exec sdbuilder --help",
        "test": "mkdir -p tests/_output && pnpm test:build && pnpm test:render",
        "test:build": "/Users/gilliantunney/Substance\\ 12/Adobe\\ Substance\\ 3D\\ Designer.app/Contents/MacOS/sbscooker  --inputs ./tests/*.sbs --includes /Users/gilliantunney/Substance\\ 12/packages --quiet --size-limit 13 --output-path ./tests/_output",
        "test:render": "/Users/gilliantunney/Substance\\ 12/Adobe\\ Substance\\ 3D\\ Designer.app/Contents/MacOS/sbsrender render --inputs ./tests/_output/*.sbsar  --output-path ./tests/_output",
        "\n========== PUBLISH >> ==========": "",
        "release": "pnpm prerelease && pnpm publish --access public --report-summary --no-git-checks",
        "prerelease": "pnpm clean && pnpm build && pnpm run docs && pnpm fix && pnpm check && pnpm test",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "tsc --build --clean",
        "clean:build": "rm -R -f _temp && rm -R -f dist && rm -R -f tests/_output",
        "clean:autosave": "pnpm autosave:list && pnpm run autosave:rm",
        "autosave:list": "find . -name '.autosave' -type d -prune",
        "autosave:rm": "find . -name '.autosave' -type d -prune -exec rm -rf '{}' +",
        "\n========== CODE STYLE >> ==========": "",
        "prettier": "cross-var prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions} --write --ignore-path .gitignore",
        "\n========== CHECK >> ==========": "",
        "check": "pnpm build:ts && pnpm clean:ts",
        "\n========== FIX >> ==========": "",
        "fix": "pnpm prettier"
    },
    "license": "MIT",
    "author": {
        "name": "Gillian Tunney",
        "email": "gbtunney@mac.com"
    },
    "devDependencies": {
        "@gbt/sat-scripts": "workspace:^0.0.0",
        "@prettier/plugin-xml": "^2.2.0",
        "@snailicide/build-config": "^0.2.5",
        "@snailicide/g-library": "^0.1.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "python-shell": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "config": {
        "batchtools": {
            "path": "/Users/gilliantunney/Substance\\ 12/Adobe\\ Substance\\ 3D\\ Designer.app/Contents/MacOS/sbscooker"
        },
        "eslint": {
            "extensions": "ts,js,cjs,mjs",
            "path": ".es*,*,**/*",
            "config": ".eslintrc.cjs"
        },
        "prettier": {
            "extensions": "sbs,ts,js,cjs,mjs,cts,json,yaml,yml,graphql,md",
            "path": "*,**/*"
        }
    }
}
